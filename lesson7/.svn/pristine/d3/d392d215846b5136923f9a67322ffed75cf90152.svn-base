	width: 100%;
	bottom: .266667rem;
	left: 50%;
	margin-left: -50%;
	z-index: 10;
	text-align: center;
}
.focus .hd ul{
}
.focus .hd ul li{
	margin: 0 .21333rem;
	text-align: center;
	line-height: 0;
	font-size: 0;
