*{
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    outline: 0;
    margin: 0;
    padding: 0;
    -webkit-tap-highlight-color: transparent;
    font-family: 'fontNameRegular',microsoft yahei,arial;
}
/* 内外边距通常让各个浏览器样式的表现位置不同 */
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,form,fieldset,legend,input,textarea,p,blockquote,th,td,hr,button,figure,footer,header,hgroup,menu,nav,section {
    margin:0;
    padding:0;
}
/* 要注意表单元素并不继承父级 font 的问题 */
body,button,input,select,textarea {
	font-family:'fontNameRegular',microsoft yahei;
	color:#222222;
}
input,select,button,textarea {
    font-size:100%;
	-webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
    border: none;
}
/* 去除默认边框 */
img {
    border:0;
	display:block;
}
/* 去掉 firefox 下此元素的边框 */
abbr,acronym {
    border:none;
    font-variant:normal;
}
/* 去掉列表前的标识，li 会继承 */
ol,ul {
    list-style:none;
}
i, em, var{
	font-style: normal;
}
/* 对齐是排版最重要的因素，别让什么都居中 */
caption,th {
    text-align:left;
}

button{
	overflow: visible
}
button, html input[type=button], input[type=reset], input[type=submit] {
	-webkit-appearance: button;
	cursor: pointer;
}

/* 来自yahoo，让标题都自定义，适应多个系统应用 */
h1,h2,h3,h4,h5,h6 {
    font-size:100%;
    font-weight:normal;
}
/* A链接*/
a {color:#222222; text-decoration:none;}
a:active{
	outline: 0;
}
a:focus.input:focus {
	outline: none;
}
.box-pb{
	padding-bottom:1.32rem;
}
.topShare{
	position: absolute;
    top: 0;
    right: 0;
    width: 1.15rem;
    height: 1.15rem;
    line-height: 1.15rem;
}
.topShare:before{
	content:"";
	display:block;
	width:.573333rem;
	height:.56rem;
	background:url(../images/topShare.png) no-repeat;
	background-size:100%;
	margin:.32rem .32rem 0 0 ;
}

#load-box{
	background-color:#733c93;
	position:fixed;
	z-index:999;
	width:100%;
	height:100%;
}
.toTop{
	position:fixed;
	right:.32rem;
    bottom: 1.48rem;
	z-index:9999;
	width:1.24rem;
	height:1.24rem;
	display:block;
	background:url(../images/toTop.png) no-repeat;
	background-size:100%;
}

/* 分享 */
.mask {
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.6);
    opacity: 0;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 222;
	display:none;
}
.topShare i{
	color:#a7a7a7;
	font-size:.52rem;
}
.bottomShare{
	background-color:rgba(0,0,0,.6);
	position:fixed;
	left:0;
	bottom:-100%;
	-o-transition: all .5s ease;
	-ms-transition: all .5s ease;
	-webkit-transition: all .5s ease;
	transition: all .5s ease;
	-moz-transition: all .5s ease;
	background-color:#e0e0e0;
	padding:.56rem;
	z-index:333;
}
.bottomShare .cancelShare{
	display:block;
	border:1px solid #666666;
	color:#232323;
	height:1.22rem;
	line-height:1.22rem;
	width:100%;
	text-align:center;
	border-radius:1rem;
	font-size:.48rem;
	margin-top:1rem;
}
.bottomShare ul{
	display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    position: relative;
	padding:0 1.2rem;
}
.bottomShare li{
	display: block;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	flex: 1;
	padding:0 .3rem;
	text-align: center;
}
.bottomShare li a{
}
.bottomShare li a img{
	width:100%;
}

/* panic */
.panic{
	padding:.266rem .32rem 0;
	border-bottom:.2666rem solid #f7f7f7;
}
.panic a{
	width:100%;
	display:block;
	padding-bottom:.266rem;
	position:relative;
}
.panic a img{
	width:100%;
}
.panic a span{
	width:100%;
	height:.614rem;
	line-height:.614rem;
	background-color:rgba(0,0,0,.8);
	color:#fff;
	position:absolute;
	top:0;
	left:0;
	text-align:center;
	font-size:.32rem;
	vertical-align:middle;
}
.panic a span i{
	vertical-align:middle;
	display:inline-block;
	height:.614rem;
	padding-top:.08rem;
}
.panic a span img{
	display:inline;
	width:.38rem;
	height:.38rem;
	margin-right:.1rem;
}

/* 清除浮动 */
.clear {
    clear:both;
    display:block;
    font-size:0;
    height:0;
    line-height:0;
    overflow:hidden;
}
.clearfix:after {
    content: " ";
	display: block;
	visibility: hidden;
	height: 0;
	clear: both;
}
/* 设置显示和隐藏，通常用来与 js 配合 */
.hide {
    display:none;
}
.block {
    display:block;
}
/* 设置浮动，减少浮动带来的 bug */
.inline {
    display:inline;
}
.fb{
	font-weight:bold;
}
.left{
	float:left;
}
.right{
	float:right;
}

.b-t{
	border-bottom:1px solid #d7d7d7;
}
.b-b{
	border-bottom:1px solid #d7d7d7;
}

/* header */
header{
	width:100%;
	height:1.15rem;
	background:#fff url(../images/headerBg_2.png) 0 bottom no-repeat;
	background-size:100%;
	text-align:center;
	/*display:none;*/
}
header .title{
	font-size:.46rem;
	line-height:1.15rem;
	display:block;
	text-align:center;
}
header .logo{
	text-align:center;
}
header .logo img{
	width:2.1rem;
	display: inline-block;
	margin-top: .35rem;
	overflow: hidden;
}
header .backArrow {
    width: 1.15rem;
    height: 1.15rem;
    line-height: 100%;
    position: absolute;
    top: 0;
    left: 0;
}
header .backArrow:before{
	content: "";
	width:.586667rem;
	height:.586667rem;
	display:block;
	background:url(../images/backArrow.png) no-repeat;
	background-size:100%;
	margin:.25rem 0 0 .3rem;
}
header .indexQrcode{
	width:1.15rem;
	height:1.15rem;
	line-height:100%;
	position:absolute;
	top:0;
	left:0;
}
header .indexQrcode:before{
	content: "";
	width:.56rem;
	height:.56rem;
	display:block;
	background:url(../images/indexQrcode.png) no-repeat;
	background-size:100%;
	margin:.32rem 0 0 .32rem;
}
header .indexSearch{
	position:absolute;
	top:0;
	right:0;
	width:1.15rem;
	height:1.15rem;
	text-align:right;
}
header .indexSearch:before{
	content: "";
	width:.614rem;
	height:.614rem;
	display:block;
	background:url(../images/indexSearch.png) no-repeat;
	background-size:100%;
	margin:.32rem .32rem 0 0;
}

/* topNav */
nav.topNav{
	width:100%;
	height:1.2rem;
	overflow:hidden;
	overflow-x:auto;
}
nav.topNav ul{
	height:1.2rem;
}
nav.topNav ul li{
	float:left;
	padding:.35rem 0 .05rem;
}
nav.topNav ul li a{
	display:block;
	text-align:center;
}
nav.topNav ul li a i{
	padding-bottom:.1rem;
}
nav.topNav ul li a.nav_hover i{
	color:#801da1;
	border-bottom:1px solid #801da1;
}

/* bottomNav */
nav.bottomNav{
	width:100%;
	height:1.38rem;
	overflow:hidden;
	overflow-x:auto;
	border-top:1px solid #d7d7d7;
	position:fixed;
	bottom:0;
	left:0;
	background-color:#fff;
	z-index:990;
}
nav.bottomNav ul{
	height:1.38rem;
}
nav.bottomNav ul li{
	float:left;
	padding:.18rem 0 .05rem;
	width:20%;
}
nav.bottomNav ul li a{
	display:block;
	text-align:center;
	font-size:.27rem;
}
nav.bottomNav ul li i{
	width:26px;
	height:21px;
	display:block;
	margin:0 auto;
	background-image:url(../images/botNav.png);
}
nav.bottomNav ul li:nth-child(1) i{
	background-position:0 0;
	background-size:100%;
}
nav.bottomNav ul li:nth-child(1) a.nav_hover i{
	background-position:0 -21px;
	background-size:100%;
}
nav.bottomNav ul li:nth-child(2) i{
	background-position:0 -42px;
	background-size:100%;
}
nav.bottomNav ul li:nth-child(2) a.nav_hover i{
	background-position:0 -62px;
	background-size:100%;
}
nav.bottomNav ul li:nth-child(3) i{
	background-position:0 -83px;
	background-size:100%;
}
nav.bottomNav ul li:nth-child(3) a.nav_hover i{
	background-position:0 -104px;
	background-size:100%;
}
nav.bottomNav ul li:nth-child(4) i{
	background-position:0 -125px;
	background-size:100%;
}
nav.bottomNav ul li:nth-child(4) a.nav_hover i{
	background-position:0 -146px;
	background-size:100%;
}
nav.bottomNav ul li:nth-child(5) i{
	background-position:0 -168px;
	background-size:100%;
}
nav.bottomNav ul li:nth-child(5) a.nav_hover i{
	background-position:0 -189px;
	background-size:100%;
}
nav.bottomNav ul li a i{
	margin-bottom:.1rem;
}
nav.bottomNav ul li a.nav_hover{
	color:#801da1;
}

/* banner */
.focus{ width:100%; height:4.66667rem; margin:0 auto; position:relative; overflow:hidden;   }
.focus .hd{
	list-style: none;
