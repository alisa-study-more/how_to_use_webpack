<!doctype html>
<html>
<head>
<meta charset="utf-8">
<meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no" />
<meta http-equiv="Pragma" content="no-cache">
<meta http-equiv="no-cache">
<meta http-equiv="Expires" content="-1">
<meta http-equiv="Cache-Control" content="no-cache">
<meta name="format-detection" content="telephone=no">
<title>登录</title>
<script type="text/javascript" src="js/flexible.js"></script>
<link rel="stylesheet" href="css/base.css">
<link rel="stylesheet" href="css/user.css">
<script type="text/javascript" src="js/zepto.min.js"></script>
<script type="text/javascript" src="js/touch.min.js"></script>
<script type="text/javascript" src="js/common.js"></script>
</head>

<body bgcolor="#fff">
<div id="load-box"></div>
<div class="box">

	<div class="login">
    	<header>
        	<img src="images/logReg.jpg" />
        	<a href="javascript:void(0);" class="backArrow" ></a>
        </header>
        
        <div class="org-cardTitleBig">
            <div class="org-cardReBindTel">
                <em></em>
                <input type="tel" oninput="OnInput(this)" placeholder="请输入手机号码">
                <div class="reset" id="reset"><i class="iconfont icon-closefill"></i></div>
            </div>
        
            <div class="org-cardReBindPass">
                <em></em>
                <input type="password" oninput="OnInput(this)" placeholder="请输入登录密码">
                <div class="reset"><i class="iconfont icon-closefill"></i></div>
            </div>
        </div>
        <div class="logBtns clearfix">
        	<a href="#" class="left">注册账号</a>
            <a href="#" class="right">忘记密码</a>
        </div>
        
        <div class="org-qdModify">登&nbsp;录</div>
        
        <div class="otherLog">
        	<img src="images/otherLog.gif" />
            <div class="otherLink">
            	<!--<a href="javascript:void(0);" class="log_qq">
                	<em></em>
                    <span>QQ登录</span>
                </a>-->
                <a href="javascript:void(0);" class="log_weixin">
                	<em></em>
                    <span>微信登录</span>
                </a>
                <!--<a href="javascript:void(0);" class="log_weibo">
                	<em></em>
                    <span>微博登录</span>
                </a>-->
                <a href="javascript:void(0);" class="log_fast">
                	<em></em>
                    <span>手机快捷登录</span>
                </a>
            </div>
        </div>
        
    </div>

    
</div>
<script type="text/javascript" src="js/user.js"></script>
</body>
</html>
