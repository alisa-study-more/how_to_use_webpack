@charset "utf-8";
/* CSS Document */

/* 用户中心 */
.user{
}
.user header{
	width:100%;
	height:5.7rem;
	background:url(../images/userBg.jpg);
	background-size:100%;
	position:relative;
	display:block;
}
.user header .top{
	position:relative;
	padding-top:.7rem;
}
.user header .top span{
	font-size:.453333rem;
	color:#fff;
}
.user header .top a{
	position:absolute;
	right:.32rem;
	top:.64rem;
}
.user header .top i.iconfont{
	color:#fff;
	font-size:.48rem;
}
.userBtns{
	text-align:center;
	margin-top:.88rem;
}
.userBtns a{
	width:2.133333rem;
	height:.906667rem;
	line-height:.906667rem;
	border:1px solid #fff;
	color:#fff;
	display:inline-block;
	border-radius:20px;
	margin:0 .6rem;
}
.userInfo{
	margin:.6rem 1.1rem 0;
	height:1.8rem;
	line-height:1.8rem;
	position:relative;
	text-align:center;
}
.userInfo_con > img{
	width:1.8rem;
	height:1.8rem;
	border-radius:100%;
	border:.05rem solid #fff;
	position:absolute;
	left:0;
	top:0;
}
.userInfo span{
	color:#fff;
	text-align:center;
	height:1.8rem;
	line-height:1.8rem;
}
.userInfo_con{
	position:absolute;
	top:0;
	left:0;
	width:100%;
}
.userInfo_con a{
	/*display:block;*/
	width:.986667rem;
	height:.986667rem;
	position:absolute;
	right:-.2rem;
	top:.4rem;
	padding:.2rem;
}
.userInfo_con a > img{
	width:100%;
}
.userGoods{
	position:absolute;
	bottom:0;
	left:0;
	width:100%;
	height:1.4rem;
	background-color:rgba(0,0,0,.3);
	display: -webkit-box;
    display: -webkit-flex;
    display: flex;
	padding:0 .5rem;
}
.userGoods a{
	display: block;
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    text-align: center;
	color:#fff;
	position:relative;
}
.userGoods a span{
	font-size:.27rem;
}
.userGoods a em{
	display:block;
	width:.666667rem;
	height:.666667rem;
	margin:.16rem auto .07rem;
}
.userGoods a.userGoods_1 em{
	background:url(../images/userGoods_1.png) no-repeat;
	background-size:100%;
}
.userGoods a.userGoods_2 em{
	background:url(../images/userGoods_2.png) no-repeat;
	background-size:100%;
}
.userGoods a.userGoods_3 em{
	background:url(../images/userGoods_3.png) no-repeat;
	background-size:100%;
}
.userGoods a.userGoods_4 em{
	background:url(../images/userGoods_4.png) no-repeat;
	background-size:100%;
}
.userGoods a i{
	display:block;
	background-color:#e0073c;
	font-size:.24rem;
	position:absolute;
	left:1.4rem;
	top:.06rem;
	border-radius:50%;
	padding:.04rem .12rem;
}
.user .helpList {
    padding:.266667rem 0 0;
}
.user .helpList:last-child{
	padding-bottom:.266667rem;
}
.user .helpList dl {
    background-color: #fff;
    padding: .3rem .28rem;
    border-bottom: 1px solid #d7d7d7;
	height:1.16rem;
	line-height:.56rem;
}
.user .helpList a:last-child dl{
	border:0;
}
.user .helpList dl dt {
    float: left;
    width: 93%;
}
.user .helpList dl dt img{
	width:.56rem;
	height:.56rem;
	display:inline;
	margin-right:.12rem;
	vertical-align: bottom;
}
.user .helpList dl dt em.dot{
	width:.16rem;
	height:.16rem;
	display:block;
	background-color:#e0063d;
	float:right;
	border-radius:50%;
	position:relative;
	top:.18rem;

}
.user .helpList dl dd {
    width: .5rem;
    float: right;
}

/* 登录注册 */
.login {
	padding-bottom:1rem;
	background-color:#fff;
}
.login header{
	height:auto;
	background:none;
	display:block;
}
.login header img{
	width:100%;
}
.login header .backArrow:before{
	background-image:url(../images/backArrow_white.png);
}
.login .org-cardTitleBig,
.reg .org-cardTitleBig {
    line-height: 1.2rem;
    background-color: #fff;
	padding:.32rem .94rem;
}
.org-cardTitleBig .org-cardReBindPass > em{
	background: url(../images/ord_password.png) center no-repeat;
    background-size: 100%;
	top: .308rem;
}
.org-cardTitleBig .org-cardReBindPassAgin > em {
    background: url(../images/org_lockHook.png) center no-repeat;
    background-size: 100%;
	top: .28rem;
}
.logBtns{
	padding:0 .94rem;
}
.logBtns a{
	font-size:.32rem;
}
.logBtns .left{
	color:#801da1;
}
.login .org-qdModify,
.reg .org-qdModify{
	margin-top:.6rem;
}
.otherLog{
	padding:0 .93333rem;
}
.otherLog > img{
	width:100%;
	margin-bottom:.24rem;
}
.otherLink{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	position: relative;
}
.otherLink a{
    display: block;
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    text-align: center;
    font-size: .24rem;
}
.otherLink a em{
	width:1.17rem;
	height:1.17rem;
	display:block;
	margin:.24rem auto;
}
.otherLink a.log_qq em{
	background:url(../images/log_qq.gif) no-repeat;
	background-size:100%;
}
.otherLink a.log_weixin em{
	background:url(../images/log_weixin.gif) no-repeat;
	background-size:100%;
}
.otherLink a.log_weibo em{
	background:url(../images/log_weibo.gif) no-repeat;
	background-size:100%;
}
.otherLink a.log_fast em{
	background:url(../images/log_fast.gif) no-repeat;
	background-size:100%;
}

/* 手机快捷登录 */
.fastLanding{
}
.org-cardTitleBig > div{
	border-bottom:1px solid #b6b6b6;
	position:relative;
	margin-bottom:.16rem;
	height:1.1rem;
}
.org-cardTitleBig > div:after{
	content: " ";
	display: block;
	visibility: hidden;
	height: 0;
	clear: both;
}
.org-cardTitleBig > div.foucs{
	border-color:#801da1;
}
.org-cardTitleBig {
    line-height: 1.2rem;
    background-color: #fff;
	padding:.3rem 1rem;
}
.org-cardTitleBig em {
    width: .42rem;
    height: .42rem;
    margin: 0 0.26666667rem;
    display: block;
	float:left;
    vertical-align:bottom;
	position:relative;
	top: .27rem;
}
.org-cardTitleBig .org-cardReBindTel > em {
    background: url(../images/org_mobile.png) center no-repeat;
    background-size: 100%;
}
.org-cardTitleBig .org-inputCode > em {
    background: url(../images/ord_yanzheng.png) center no-repeat;
    background-size: 100%;
	top: .32rem;
}
.org-cardTitleBig input {
    border: 0px;
    color: #222222;
    padding:0;
    width:78%;
	font-size:.36rem;
	height:1rem;
	/*line-height:1rem;*/
	vertical-align: bottom;
	position: relative;
	display: block;
	float:left;
}
.org-cardTitleBig input[type=button]{
	background-color:#f5f5f5;
	width:auto;
	float:right;
	padding:0 .4rem;
	width:3.2rem;
	text-align:center;
	color:#222;
}
.org-cardTitleBig input[type=button]:disabled{
	color:#b6b6b6;
}

.org-cardTitleBig .org-inputCode input {
    width:36%;
	padding-left:0;
}
.org-cardTitleBig .reset{
	position:absolute;
	right:.18rem;
	top:-.1rem;
	display:none;
}
.org-cardTitleBig .org-cardReBindTel .reset .iconfont{
	font-size:.42rem;
}
.org-inputCode span {
    background-color: #f5f5f5;
    padding: 0 0.26666667rem;
    float: right;
    display: inline-block;
    height: 1rem;
    line-height: 1rem;
	border-radius:3px;
	width:3.2rem;
	text-align:center;
}
.org-inputCode span.reSend{
	color:#b6b6b6;
}
.org-qdModify {
    color: #ffffff;
    font-size: 0.42666667rem;
    text-align: center;
    line-height: 1.06666667rem;
    background-color: #801da1;
    border-radius: 6px;
    width: 80%;
    margin:1.2rem auto;
}

.agreement{
	padding:0 .94rem;
	line-height:.5rem;
}
.agreement span{
	padding-left:.1rem;
}
.checkboxRed {
    width:.42rem;
    height: .42rem;
    float: left;
    position: relative;
    background: url(../images/checkbox2-off.png) no-repeat center;
    background-size: 100% 100%;
	margin:0 .266667rem;
}
.checkboxRed input {
    background: none;
    width: .42rem;
    height: .42rem;
    position: relative;
    z-index: 3;
    -webkit-appearance: none;
    outline: none;
	border:0;
    /* visibility: hidden; */
}
.checkboxRed label {
    display: block;
    width: .42rem;
    height: .42rem;
    cursor: pointer;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 1;
}
.checkboxRed input[type=checkbox]:checked + label, .checkboxRed input[class=default] + label {
    background: none;
    background: url(../images/checkbox2-on.png) no-repeat center;
    background-size: 100% 100%;
}


/* 个人中心-设置 */
.setup .helpList {
    padding: .3rem 0;
}
.setup .helpList a:last-child dl{
	border-bottom:0;
}
.setup .helpList dl {
    background-color: #fff;
    padding: .3rem .28rem;
    border-bottom: 1px solid #d7d7d7;
	line-height:.56rem;
}
.setup .helpList dl dt {
    float: left;
    width: 45%;
    vertical-align: middle;
}
.setup .helpList dl dd {
    float: right;
}
.setup .helpList dl dd i.flexBtn{
	width: 44px;
	height: 23px;
	display: block;
	background: url(../images/showOff.png) 0 -1px no-repeat;
	background-size: 100%;
}
.setup .helpList dl dd.onShow i {
    width: 44px;
    height: 23px;
    display: block;
    background: url(../images/showOff.png) 0 -56px no-repeat;
    background-size: 100%;
}
.helpList dl:after {
    content: " ";
    display: block;
    visibility: hidden;
    height: 0;
    clear: both;
}
.setup .otherPlat{
	margin-bottom:.266667rem;
}
.setup .otherPlat > span{
	color:#666;
	font-size:.32rem;
	padding:.32rem;
	line-height:.6rem;
}
.setup .platList{
	background-color:#fff;
	padding:.32rem .32rem .32rem .14rem;
}
.setup .platList a{
	float:left;
	margin:0 .18rem;
}
.setup .platList a img{
	width:1.28rem;
	height:1.28rem;
	margin:0 auto;
	border:1px solid #d7d7d7;
	border-radius:3px;
}
.setup .platList a span{
	font-size:.27rem;
	display:block;
	text-align:center;
	margin-top:.12rem;
}
.logOut{
	color:#801da1;
	font-size:.4rem;
	line-height:1.2rem;
	text-align:center;
	background-color:#fff;
	display:block;
}

.qrShow{
	position:fixed;
	left:0;
	top:0;
	z-index:991;
	display:none;
}
.qrShow .mask{
	opacity:1;
	display:block;
}
.qrShow .con{
	width:7.12rem;
	height:8.2rem;
	position:absolute;
	left:50%;
	top:50%;
	margin-left:-3.56rem;
	margin-top:-4.1rem;
	border-radius:5px;
	z-index:992;
}
.qrShow .con .head{
	height:1.2rem;
	background-color:#801fa0;
	border-radius:5px 5px 0 0;
	position:relative;
}
.qrShow .con .head .text{
	font-size:.4rem;
	text-align:center;
	display:block;
	line-height:1.2rem;
	color:#fff;
}
.qrShow .con .head .close{
	position:absolute;
	top:.22rem;
	right:.4rem;
	display:block;
	width:.72rem;
	height:.72rem;
}
.qrShow .con .head .close:before{
	content:"";
	width:.72rem;
	height:.72rem;
	background:url(../images/qr_close.png) no-repeat;
	background-size:100%;
	display:block;
}
.qrShow .con .qrShow_con{
	border-radius:0 0 5px 5px;
	background-color:#fff;
	padding:.5rem;
}
.qrShow .con .qrShow_con .qr_pic{
	width:100%;
}
.qrShow .con .qrShow_con .qr_pic img{
	width:5rem;
	margin:0 auto;
}
.qrShow .con .qrShow_con .desc{
	text-align:center;
	color:#801fa0;
	font-size:.38rem;
	margin-top:.5rem;
}


